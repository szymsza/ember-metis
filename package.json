{
  "name": "ember-metis",
  "version": "0.11.0",
  "description": "Default subject pages for your Linked Data resources.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redpencilio/ember-metis.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "overrides": {
    "inputmask": "^5.0.9-beta.45"
  },
  "overridesComments": {
    "inputmask": "5.0.8 is incompatible with FastBoot, remove the override once inputmask ships a new stable version, or when Appuniversum depends on the beta as well: https://github.com/appuniversum/ember-appuniversum/issues/462"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "build-url": "^6.0.1",
    "chalk": "^4.1.2",
    "ember-auto-import": "^2.7.2",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^7.0.2",
    "ember-truth-helpers": "^4.0.3",
    "recast": "^0.18.10"
  },
  "dependenciesComments": {
    "chalk": "chalk v5 is an esm-only package which we can't use in our node code since they are cjs modules"
  },
  "peerDependencies": {
    "@appuniversum/ember-appuniversum": "^3.0.0",
    "ember-cli-fastboot": "^4.1.2",
    "ember-source": ">= 4.0.0"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^3.8.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@ember/optional-features": "^2.1.0",
    "@ember/test-helpers": "^3.3.0",
    "@embroider/test-setup": "^3.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.7.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.2",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.1.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.2.5",
    "qunit": "^2.20.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.5.0",
    "sass": "^1.21.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "svgxuse": "^1.2.6",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
